{#
/**
 * @file authorize_transaction.html.twig
 * Default theme implementation to present Authorize transaction data.
 *
 * This template is used when viewing Authorize transaction pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_authorize_transaction()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('authorize_transaction') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
